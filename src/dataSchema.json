{
  "type": "object",
  "properties": {
    "booleanData": {
      "type": "boolean",
      "title": "Boolean Data"
    },
    "stringData": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20,
      "pattern": "^[a-z0-9]+$",
      "title": "String Data",
      "default": "default"
    },
    "numberData": {
      "type": "number",
      "minimum": 0,
      "maximum": 100,
      "title": "Number Data"
    },
    "selectedCategory": {
      "type": "string",
      "enum": [
        "Option 1",
        "Option 2",
        "Option 3"
      ],
      "title": "Category Data",
      "default": "Option 1"
    },
    "objectData": {
      "type": "object",
      "title": "Test Object",
      "properties": {
        "subBoolean": {
          "type": "boolean",
          "title": "Sub Boolean"
        },
        "subString": {
          "type": "string",
          "minLength": 3,
          "maxLength": 15,
          "title": "Sub String",
          "default": "hello"
        },
        "subNumber": {
          "type": "number",
          "minimum": 0,
          "maximum": 50,
          "title": "Sub Number"
        },
        "subCategory": {
          "type": "string",
          "enum": [
            "Option 1",
            "Option 2",
            "Option 3"
          ],
          "title": "Sub Category"
        }
      },
      "required": [
        "subBoolean",
        "subString"
      ]
    },
    "ArrayData": {
      "type": "array",
      "title": "Test Array",
      "items": {
        "type": "object",  
        "properties": {
          "subString": {
            "type": "string",
            "minLength": 3,
            "maxLength": 15,
            "title": "Sub String",
            "default": "example",
            "pattern": "^[a-z0-9]+$"
          },
          "subNumber": {
            "type": "number",
            "minimum": 0,
            "maximum": 50,
            "title": "Sub Number",
            "default": 0
          },
          "subBoolean": {
            "type": "boolean",
            "title": "Sub Boolean"
          }
        }
      }
    }
  },
  "required": [
    "booleanData",
    "stringData",
    "numberData",
    "selectedCategory",
    "objectData" 
  ]
}
